package com.pbl3.model;

public class VocabListsModel {
	private int ListID;
	private int UserID;
	private int CourseID;
	private String NameList;
	private String Description;
	
	public VocabListsModel () {
		
	}
	
	public VocabListsModel(int userID, int courseID, String nameList, String description) {
		super();
		UserID = userID;
		CourseID = courseID;
		NameList = nameList;
		Description = description;
	}
	public VocabListsModel(int listID, int userID, int courseID, String nameList, String description) {
		super();
		ListID = listID;
		UserID = userID;
		CourseID = courseID;
		NameList = nameList;
		Description = description;
	}
	public int getListID() {
		return ListID;
	}
	public void setListID(int listID) {
		ListID = listID;
	}
	public int getUserID() {
		return UserID;
	}
	public void setUserID(int userID) {
		UserID = userID;
	}
	public int getCourseID() {
		return CourseID;
	}
	public void setCourseID(int courseID) {
		CourseID = courseID;
	}
	public String getNameList() {
		return NameList;
	}
	public void setNameList(String nameList) {
		NameList = nameList;
	}
	public String getDescription() {
		return Description;
	}
	public void setDescription(String description) {
		Description = description;
	}
	
	
}	
