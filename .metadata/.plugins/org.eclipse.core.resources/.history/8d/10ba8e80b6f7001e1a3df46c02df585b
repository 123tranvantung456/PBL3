package com.pbl3.libs;

public class Result {
	private int score;
	private int rightAnswer;
	private int corectAnswer;
	private int skip;
	
	public Result(int score, int rightAnswer, int corectAnswer, int skip) {
		super();
		this.score = score;
		this.rightAnswer = rightAnswer;
		this.corectAnswer = corectAnswer;
		this.skip = skip;
	}

	public int getScore() {
		return score;
	}

	public void setScore(int score) {
		this.score = score;
	}

	public int getRightAnswer() {
		return rightAnswer;
	}

	public void setRightAnswer(int rightAnswer) {
		this.rightAnswer = rightAnswer;
	}

	public int getCorectAnswer() {
		return corectAnswer;
	}

	public void setCorectAnswer(int corectAnswer) {
		this.corectAnswer = corectAnswer;
	}

	public int getSkip() {
		return skip;
	}

	public void setSkip(int skip) {
		this.skip = skip;
	}
	
}
