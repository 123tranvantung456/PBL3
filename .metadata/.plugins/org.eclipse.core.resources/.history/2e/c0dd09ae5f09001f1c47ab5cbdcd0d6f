package com.pbl3.libs;

public class Pair<K, V> {
    private K first;
    private V second;

    public Pair(K first, V second) {
        this.first = first;
        this.second = second;
    }

    public K getFirst() {
        return first;
    }

    public V getSecond() {
        return second;
    }
}
 
// dki khoa hoc, dis

// 1 aaa
// 2 bbb

// u1 c1 1tr 0
// u2 c1 1tr 1 
